<%= form_with(model:[@user, @tw], local: true) do |f| %>
    <%= render 'shared/error_messages', object: @tw %>

    <%= f.label :now_body_weight, class: "label" %><br>
    <%= f.number_field :now_body_weight, step: '0.1', class: "form-control" %><br>
    
    <%= f.label :goal_body_weight, class: "label" %><br>
    <%= f.number_field :goal_body_weight, step: '0.1', class: "form-control" %><br>
    
    <%= f.label :now_bodyfat_percentage, class: "label" %><br>
    <%= f.number_field :now_bodyfat_percentage, step: '0.1', class: "form-control" %><br>

    <%= f.label :goal_bodyfat_percentage, class: "label" %><br>
    <%= f.number_field :goal_bodyfat_percentage, step: '0.1', class: "form-control" %><br>

    <%= f.label :beginning_date, class: "label" %><br>
    <%= f.date_field :beginning_date, class: "form-control" %><br>
    
    <%= f.label :target_date, class: "label" %><br>
    <%= f.date_field :target_date, class: "form-control" %><br>

    <%= f.hidden_field :user_id, value: @user.id %>
    
    
    
  <% end %>